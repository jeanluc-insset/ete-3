<?xml version="1.0" encoding="UTF-8"?>


<mda>

    <!--
    Description :

    This project builds xlang_builder-api and xlang_builder-impl projects.
    The last project, xlang_builder_effective is writren by hand
    -->

    <transformation-set output_base="../../">
        <!-- reads a model -->
        <model  url="xlang-builder.xml"/>

        <!-- Generate interfaces into the xlang_builder-api project and the
            classes into the xlang_builder-impl project
          -->
        <module src="../../../../../src/main/mda/modules/from-spec/ete-config.xml"
                    dialect="fr.insset.jeanluc.el.dialect.BasicJavaDialect"
                    projectName="xlang_builder"/>
            />

    </transformation-set>

</mda>



