#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>


<mda>


    <transformation-set >
        <model  url="models/NaturalLanguage.xml"/>

        <!--
          - The context parameter specifies which context to use when
          - resolving an identifier
        -->
        <transformation-set output_base="../../"
                            projectName="${parentArtifactId}"
                            context="mof">

                    <!--API and default implementation generation--> 
            <velocity template="modules/impl/java/tree-builder.vm"
                target="${rootArtifactId}-impl/target/generated-sources/${package}/impl/${symbol_dollar}{dialect.i2uc(projectName)}TreeBuilder.java"/>

        </transformation-set>


    </transformation-set>

</mda>



