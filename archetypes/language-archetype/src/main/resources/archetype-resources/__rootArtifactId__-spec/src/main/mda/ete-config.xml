#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>


<mda>


    <transformation-set >
                              read the model
        <!--<model  url="models/EssentialOCL.emof"/>-->
        <model  url="models/${artifactId}.xml"/>

        <!--
          - The context parameter specifies which context to use when
          - resolving an identifier
        -->
        <transformation-set output_base="../../"
                            context="mof">

                    <!--API and default implementation generation--> 
            <module src="../../../../../src/main/mda/modules/language/ete-config.xml"
                    dialect="fr.insset.ete.el.dialect.BasicJavaDialect"
                    projectName="${rootArtifactId}"/>

        </transformation-set>


    </transformation-set>

</mda>



